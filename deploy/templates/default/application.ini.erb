[production]
resources.db.adapter = "mysqli"
resources.db.params.prefix = "hp_"
resources.db.params.host = "<%= @host%>"
resources.db.params.username = "<%= @username%>"
resources.db.params.password = "<%= @password%>"
resources.db.params.dbname = "<%= @database%>"
resources.db.params.charset = "utf8"
resources.db.isDefaultTableAdapter = true

aws.credentials.access_key = "<%= @aws_accessKey%>"
aws.credentials.secret_key = "<%= @aws_secretKey%>"
aws.s3.bucket_name = "<%= @aws_s3_bucket%>"
aws.s3.path_prefix = "<%= @aws_s3_bucket_prefix%>"
aws.ses.access_key = "<%= @aws_ses_accessKey%>"
aws.ses.secret_key = "<%= @aws_ses_secretKey%>"
aws.cloudfront.endpoint = "<%= @aws_cloudfront_endpoint%>"

[development:production]
[testing:development]